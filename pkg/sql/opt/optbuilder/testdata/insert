      │    ├──  x:10 => b:2
      │    ├──  column12:12 => c:3
      │    ├──  column14:14 => d:4
      │    ├──  y:11 => e:5
      │    └──  column13:13 => rowid:6
      └── project
           ├── columns: column14:14(int) x:10(int) y:11(int) column12:12(int!null) column13:13(int)
           ├── project
           │    ├── columns: column12:12(int!null) column13:13(int) x:10(int) y:11(int)
           │    ├── project
           │    │    ├── columns: x:10(int) y:11(int)
           │    │    ├── scan xyz
           │    │    │    └── columns: xyz.x:7(string!null) xyz.y:8(int) z:9(float)
           │    │    └── projections
           │    │         ├── cast: INT8 [type=int]
           │    │         │    └── variable: xyz.x [type=string]
