      ├── project
      │    ├── columns: column8:8(int) column9:9(int!null) column10:10(int) column1:7(int)
      │    ├── values
      │    │    ├── columns: column1:7(int)
      │    │    └── tuple [type=tuple{int}]
      │    │         └── const: 1 [type=int]
      │    └── projections
      │         ├── cast: INT8 [type=int]
      │         │    └── null [type=unknown]
      │         ├── const: 10 [type=int]
      │         └── function: unique_rowid [type=int]
      └── projections
           └── plus [type=int]
                ├── plus [type=int]
                │    ├── variable: column8 [type=int]
                │    └── variable: column9 [type=int]
                └── const: 1 [type=int]

# Ordered input.
build
INSERT INTO abcde SELECT y FROM xyz ORDER BY y, z LIMIT 10
----
insert abcde
 ├── columns: <none>
 ├── insert-mapping:
 │    ├──  y:10 => a:1
 │    ├──  column11:11 => b:2
 │    ├──  column12:12 => c:3
 │    ├──  column14:14 => d:4
 │    ├──  y:10 => e:5
 │    └──  column13:13 => rowid:6
 └── project
      ├── columns: column14:14(int) y:10(int) column11:11(int) column12:12(int!null) column13:13(int)
      ├── project
      │    ├── columns: column11:11(int) column12:12(int!null) column13:13(int) y:10(int)
      │    ├── limit
      │    │    ├── columns: z:9(float) y:10(int)
      │    │    ├── internal-ordering: +10,+9
      │    │    ├── sort
      │    │    │    ├── columns: z:9(float) y:10(int)
      │    │    │    ├── ordering: +10,+9
      │    │    │    └── project
      │    │    │         ├── columns: y:10(int) z:9(float)
      │    │    │         ├── scan xyz
      │    │    │         │    └── columns: x:7(string!null) xyz.y:8(int) z:9(float)
      │    │    │         └── projections
      │    │    │              └── cast: INT8 [type=int]
      │    │    │                   └── variable: xyz.y [type=int]
      │    │    └── const: 10 [type=int]
      │    └── projections
      │         ├── cast: INT8 [type=int]
      │         │    └── null [type=unknown]
      │         ├── const: 10 [type=int]
      │         └── function: unique_rowid [type=int]
      └── projections
           └── plus [type=int]
                ├── plus [type=int]
                │    ├── variable: column11 [type=int]
                │    └── variable: column12 [type=int]
                └── const: 1 [type=int]

