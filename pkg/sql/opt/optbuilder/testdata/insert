 ├── columns: <none>
 ├── insert-mapping:
 │    ├──  column1:7 => a:1
 │    ├──  column8:8 => b:2
 │    ├──  column9:9 => c:3
 │    ├──  column11:11 => d:4
 │    ├──  column1:7 => e:5
 │    └──  column10:10 => rowid:6
 └── project
